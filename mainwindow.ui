   <string notr="true">/* MainWindow */
QMainWindow {
    background-color: #f0f0f0;
}

/* QPushButton */
QPushButton {
    background-color: #5b9bd5;
    color: white;
    border-radius: 5px;
    padding: 5px;
    font-size: 14px;
}

QPushButton:hover {
    background-color: #4a86c5;
}

QPushButton:pressed {
    background-color: #3a76b5;
}

/* QLineEdit */
QLineEdit {
    border: 1px solid #5b9bd5;
    border-radius: 3px;
    padding: 4px;
    font-size: 14px;
}

/* QLabel */
QLabel {
    color: #333333;
    font-size: 14px;
}

/* Scroll Area for QLabel */
QScrollArea {
    border: 1px solid #d0d0d0;
    border-radius: 4px;
    background-color: #ffffff;
}

/* QProgressBar */
QProgressBar {
    border: 1px solid #5b9bd5;
    border-radius: 5px;
    background-color: #f0f0f0;
}

QProgressBar::chunk {
    background-color: #5b9bd5;
    width: 10px;
    margin: 1px;
}

/* QMenuBar */
QMenuBar {
    background-color: #e7e7e7;
}

QMenuBar::item {
    spacing: 3px; 
    padding: 5px 15px;
    background: transparent;
    color: #000000;
}

QMenuBar::item:selected {
    background-color: #5b9bd5;
    color: white;
}

/* QStatusBar */
QStatusBar {
    background-color: #e7e7e7;
    color: #333333;
    border-top: 1px solid #c0c0c0;
}
</string>
